@#!/bin/bash

x="../puzzles/p"
y="0"
z=".pz"

LIMIT=81
for((i=0; i <= LIMIT; i++))
 do
	if [ $i -lt 10 ]
	then
		tail $x$y$i$z | ./a.out -s
	else
		tail $x$i$z | ./a.out -s
	fi
done
@
